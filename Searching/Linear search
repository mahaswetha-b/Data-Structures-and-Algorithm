#include <stdio.h>
int main()
{
 int n,i,flag;
 printf("Enter size:");
 scanf("%d",&n);
 int arr[n];
 printf("Enter element:");
 for(i=0;i<n;i++)
 {
 scanf("%d",&arr[i]);
 }
 int element;
 printf("Enter element to search:");
 scanf("%d",&element);
 for(i=0;i<n;i++)
 {
 if(element==arr[i])
 {
 flag=1;
 break;
 }
 }
 if(flag==1)
 {
 printf("Array :%d",i+1);
 }
 else
 {
 printf("Not found");
 }
}
