#include <stdio.h>
int main()
{
 int n,i,key;
 scanf("%d",&n);
 int arr[n];
 printf("Enter elemens:");
 for(i=0;i<n;i++)
 {
 scanf("%d",&arr[i]);
 }
 printf("Enter array element to search:");
 scanf("%d",&key);
 int low=0,high=n-1,mid,found;
 while(low<=high)
 {
 mid=(low+high)/2;
 if(arr[mid]==key)
 {
 printf("Element is found at position:%d\n",mid+1);
 found=1;
 break;
 }
 else if(key<arr[mid])
 {
 high=mid-1; 
 }
 else
 {
 low=mid+1;
 }
 }
 if(found==0)
 {
 printf("Not found");
 }
}
